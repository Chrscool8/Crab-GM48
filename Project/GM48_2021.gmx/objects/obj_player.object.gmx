            <kind>1</kind>
            <string>in_water = !place_free(x, y);
var modi = (1-(in_water*.75));

hsp = (key_right - key_left) * player_speed * modi;
vsp = (key_down - key_up)    * player_speed * modi;

if (hsp != 0 and vsp != 0)
{
    hsp = sign(hsp)*(player_speed * modi / sqrt(2));
    vsp = sign(vsp)*(player_speed * modi / sqrt(2));
}

x += hsp;
y += vsp;

if (hsp != 0 or vsp != 0)
{
    image_speed = .25;
}
else
{
    image_speed = 0;
    image_index = 0;
}

if (vsp &lt; 0)
{
    if (hsp &lt; 0)
    {
        sprite_index = spr_player_new_upleft;
        dir = 90+45;
    }
    else if (hsp == 0)
    {
        sprite_index = spr_player_new_up;
        dir = 90;
    }
    else if (hsp &gt; 0)
    {
        sprite_index = spr_player_new_upright
        dir = 90-45;
    }
}
else if (vsp &gt; 0)
{
    if (hsp &lt; 0)
    {
        sprite_index = spr_player_new_downleft;
        dir = 270-45;
    }
    else if (hsp == 0)
    {
        sprite_index = spr_player_new_down;
        dir = 270;
    }
    else if (hsp &gt; 0)
    {
        sprite_index = spr_player_new_downright
        dir = 270+45;
    }
}
else
{
    if (hsp &gt; 0)
    {
        sprite_index = spr_player_new_right;
        dir = 0;
    }
    else
    if (hsp &lt; 0)
    {
        sprite_index = spr_player_new_left;
        dir = 180;
    }
}

depth = -y;

wrap();
